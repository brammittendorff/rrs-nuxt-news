{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "dev:worker": "wrangler dev --env development",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:worker\"",
    "deploy:worker": "wrangler deploy --env production",
    "deploy:pages": "npm run generate && wrangler pages deploy .output/public",
    "deploy": "npm run deploy:worker && npm run deploy:pages"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.9.0",
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "bootstrap-vue-next": "^0.26.18",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fast-xml-parser": "^4.5.1",
    "h3": "^1.13.0",
    "nuxt": "^3.15.0",
    "pinia": "^2.3.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241224.0",
    "concurrently": "^9.1.1",
    "vitest": "^2.1.8",
    "wrangler": "^3.99.0"
  }
}
